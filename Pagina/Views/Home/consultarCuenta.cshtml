@model IEnumerable<Entidades.uso>

@{
    ViewBag.Title = "consultarCuenta";
}

<table class="table">
    <tr>
        <th>
            <h4>Cuenta</h4>
        </th>
        <th>
            <h4>Accion</h4>
        </th>
        <th>
            <h4>Monto</h4>
        </th>
        <th>
            <h4>Fecha</h4>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Cuenta)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.accion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.monto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fecha)
        </td>
    </tr>
}
</table>
