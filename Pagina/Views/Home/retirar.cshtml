@model Entidades.cuenta

@{
    ViewBag.Title = "retirar";

}


    @using (Html.BeginForm()) 
    {
        @Html.AntiForgeryToken()
        
    <div class="form-horizontal">

        <div class="form-group">
            <h5>Monto a retirar:</h5>            
            <div class="col-md-10">
                @Html.EditorFor(model => model.cSaldo, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.cSaldo, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </div>
    </div>
    }
