@model IEnumerable<Entidades.prestamo>

@{
    ViewBag.Title = "consultarGenteP";
}


    <table class="table">
        <tr>
            <th>
                <h1>Cuenta</h1>
            </th>
            <th>
                <h1>Monto</h1>
            </th>
            <th>
                <h1>Pagado</h1>
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.cuenta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.monto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.montoPagado)
            </td>
            <td>
                @Html.ActionLink("Edit", "EditarrrP", new { id=item.id }) |
                @Html.ActionLink("Delete", "eliminarP", new { id=item.id })
            </td>
        </tr>
    }
    
    </table>